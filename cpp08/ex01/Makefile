CPPFLAGS = -Wall -Wextra -Werror -std=c++11

HDR = Span.hpp
SRCS = main.cpp Span.cpp
TARGET = Span

all: $(TARGET)

$(TARGET): $(SRCS) $(HDR)
	$(CPP) $(CPPFLAGS) -o $(TARGET) $(SRCS)

clean:
	rm -f *.o

fclean: clean
	rm -f $(TARGET)

